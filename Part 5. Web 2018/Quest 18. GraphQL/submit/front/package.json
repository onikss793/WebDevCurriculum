{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "preset": "jest-puppeteer",
    "globals": {
      "URL": "http://localhost:8080",
      "testMatch": [
        "__test__/*"
      ]
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "vue": "^2.6.11",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/puppeteer": "^2.0.1",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^25.5.0",
    "jest-puppeteer": "^4.4.0",
    "puppeteer": "^3.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
